#include <stdio.h>

void main() {
  int ch, i = 0, length = 0;
  int wordsLength[5];
  char words[5][100];
  int wordsCounts = 5;

  int counter = wordsCounts;
  while (counter > 0) {
    ch = getchar();
    if (ch != ' ') {
      words[i][length] = ch;
      length++;
    } else {
      wordsLength[i] = length;
      i++;
      length = 0;
      counter--;
    }
  }

  for (int i = 0; i < wordsCounts; i++) {
    for (int j = 0; j < 5; j++) {
      if (words[i][j] != '\0')
	printf("%c", words[i][j]);
      else
	printf(" ")
    }
    printf("\n");
  }
}
